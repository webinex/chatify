    "parcel": "^2.10.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.20.2",
    "rollup-plugin-copy": "^3.4.0",
