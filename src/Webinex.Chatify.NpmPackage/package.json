{
  "name": "@webinex/chatify",
  "description": "Webinex Chatify",
  "private": false,
  "version": "2.0.0-alpha4",
  "author": "Webinex Dev",
  "license": "Apache-2.0",
  "main": "src/index.ts",
  "targets": {
    "demo": {
      "source": "./example/index.html",
      "isLibrary": false
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rimraf dist && rimraf add && rollup -c --environment ENV:production --failAfterWarnings",
    "start": "rollup -c -w",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "parcel": "parcel --target demo --dist-dir ./dist/demo",
    "parcel:build": "rimraf ../Webinex.Chatify.Example/wwwroot && cross-env NODE_ENV=production parcel build --target demo --dist-dir ../Webinex.Chatify.Example/wwwroot",
    "build:types": "dts-bundle-generator --project tsconfig.json --out dist/index.d.ts"
  },
  "devDependencies": {
    "@microsoft/signalr": "^8.0.0",
    "@parcel/transformer-sass": "2.9.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-eslint": "^9.0.3",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/is-hotkey": "^0.1.10",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@webinex/flippo": "^1.0.0",
    "antd": "^5.19.0",
    "axios": "^1.6.2",
    "buffer": "^5.5.0||^6.0.0",
    "classnames": "^2.3.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.7",
    "dts-bundle-generator": "^9.5.1",
    "eslint": "^8.38.0",
    "eslint-config-react-app": "^7.0.1",
    "immer": "^10.0.3",
    "is-hotkey": "^0.2.0",
    "parcel": "^2.10.3",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "rimraf": "^5.0.5",
    "rollup": "^3.20.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "rollup-plugin-visualizer": "^5.11.0",
    "rollup-plugin-watch-globs": "^2.0.1",
    "sass": "^1.66.1",
    "shallow-equal": "^3.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=5",
    "@microsoft/signalr": ">=8",
    "@reduxjs/toolkit": ">=2",
    "@webinex/flippo": ">=1",
    "antd": ">=5",
    "axios": ">=1",
    "classnames": ">=2",
    "dayjs": ">=1.8",
    "immer": ">=8",
    "is-hotkey": ">=0",
    "react": ">=16",
    "react-dom": ">=16",
    "shallow-equal": ">=3"
  }
}
